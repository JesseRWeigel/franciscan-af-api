{
  "name": "javascript-af-api",
  "version": "0.0.1",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "babel src/ -d dist/",
    "prebuild": "rm -rf dist/",
    "format":
      "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "precommit": "lint-staged && mocha",
    "test": "mocha",
    "dev": "nodemon --exec babel-node src/start.js"
  },
  "engines": {
    "node": "> 8"
  },
  "dependencies": {
    "apollo-server-express": "^1.3.2",
    "axios": "^0.17.1",
    "connect-mongo": "^2.0.1",
    "cors": "^2.8.4",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "graphql": "^0.13.0",
    "graphql-tools": "^2.21.0",
    "merge-graphql-schemas": "^1.4.0",
    "mongoose": "^5.0.4",
    "morgan": "^1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "mocha": "^5.0.0",
    "nodemon": "^1.14.12",
    "prettier": "^1.10.2"
  },
  "license": "MIT",
  "lint-staged": {
    "*.js": [
      "prettier --trailing-comma es5 --no-semi --single-quote --write",
      "eslint --fix",
      "git add"
    ]
  }
}
